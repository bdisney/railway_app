<h1>Routes</h1>

<div class="row">
  <div class="col-md-12">
    <%= link_to t('common.add'), new_admin_route_path,
                class: '', role: 'button' %>
  </div>
</div>

<table class="table text-center table-hover">
  <thead class="table-inverse text-center">
    <tr>
      <th class="text-center">Name</th>
      <th class="text-center">Actions</th>
    </tr>
  </thead>

  <tbody>
    <%= render @routes %>
  </tbody>
</table>
